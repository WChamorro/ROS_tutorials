<?xml version="1.0" ?>

<launch>
  <node pkg="tf2_ros" type="static_transform_publisher" name="tf_cam" args="0, 0.1, 0.2, -0.6532815, 0.6532815, -0.2705981, 0.2705981   world cam" />
  
   <node pkg="tf2_ros" type="static_transform_publisher" name="tf_robot" args="0.5 0.5 0 0 0 1 0 world base_link" />
  <!--
   <node pkg="tf2_ros" type="static_transform_publisher" name="tf_marker" args="0.3, 0.15, 0, 0, 0, 0, 1   world m_100" /> -->
   
   <param name="robot_description" textfile="$(find arm_4dof)/urdf/arm_4dof_description_base.xacro" />
   
   <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" /> 
   <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  
   <node name="servo_sim" pkg="servo_simulation" type="servo_sim.py" output="screen"/>
   

   
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find servo_simulation)/robot.rviz" /> 
   
   
</launch>
