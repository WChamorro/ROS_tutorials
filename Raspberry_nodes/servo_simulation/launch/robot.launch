<?xml version="1.0" ?>

<launch>
   <param name="robot_description" textfile="$(find arm_4dof)/urdf/arm_4dof_description_base.xacro" />
   
   <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" /> 
   <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  
   <node name="servo_sim" pkg="servo_simulation" type="servo_sim.py" output="screen"/>
   

   
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find servo_simulation)/robot.rviz" /> 
   
   
</launch>
